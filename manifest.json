{
"name": "EasyAsset",
"version": "1.0.1",
"manifest_version": 2,
"content_scripts":
    [
        {
            "matches": ["https://*.eu1.mindsphere.io/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action":
    {
        "default_popup": "popup.html",
        "default_title": "EasyAsset"
    },
    "icons":
    {
        "128": "icon.png"
    },
    "permissions":
    [
        "activeTab"
    ]   


}